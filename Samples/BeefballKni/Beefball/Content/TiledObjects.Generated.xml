<?xml version="1.0" encoding="utf-8"?>
<objecttypes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <objecttype name="Goal" />
  <objecttype name="PlayerBall">
    <property name="MovementSpeed" type="float" default="300" />
    <property name="Drag" type="float" default="1" />
    <property name="DashFrequency" type="float" default="2" />
    <property name="DashSpeed" type="float" default="600" />
    <property name="CooldownCircleRadius" type="float" default="16" />
    <property name="CircleInstanceColor" type="Color" default="White" />
    <property name="CooldownCircleColor" type="Color" default="White" />
  </objecttype>
  <objecttype name="Puck">
    <property name="Drag" type="float" default="0.4" />
  </objecttype>
  <objecttype name="ScoreHud">
    <property name="Score1" type="string" default="0" />
    <property name="Score2" type="string" default="0" />
  </objecttype>
</objecttypes>